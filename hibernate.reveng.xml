			<generator class="sequence"></generator>
		</primary-key>
	</table>
	<table name="DETAILS" schema="RESERVATION">
		<primary-key>
			<generator class="sequence"></generator>
		</primary-key>
	</table>
	<table name="CATEGORY" schema="RESERVATION">
		<primary-key>
			<generator class="sequence"></generator>
		</primary-key>
	</table>
	<table name="SHOP" schema="RESERVATION">
		<primary-key>
			<generator class="sequence"></generator>
		</primary-key>
	</table>
<table name="ADMIN" schema="RESERVATION">
	<primary-key>
		<generator class="sequence"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>